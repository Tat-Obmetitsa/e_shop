{{#each this}}
<li class="featured__list-item cart__card">
    <img class="wrapper__image" src="{{{image}}}" alt="dress" data-id={{id}} />
    <div class="wrapper__description" data-id={{id}}>
        <h3 class="wrapper__description-title" data-id={{id}}>{{name}}</h3>
        <span class="wrapper__description-text" data-id={{id}}>&dollar;{{price}}</span>
        <button type="button" class="button add-button">Add to Cart</button>
    </div>
</li>
{{/each}}