{{#each this}}
<li class="featured__list-item" data={{id}}>
    <img class="wrapper__image" src="{{{image}}}" alt="dress" data={{id}} />
    <div class="wrapper__description" data={{id}}>
        <h3 class="wrapper__description-title" data={{id}}>{{name}}</h3>
        <span class="wrapper__description-text" data={{id}}>&dollar;{{price}}</span>
        <button type="button" class="button add-button">Add to Cart</button>
    </div>
</li>
{{/each}}